{
    "breadcrumbs.enabled": false,
    "editor.fontFamily": "'Iosevka Nerd Font'",
    "editor.fontSize": 14,
    "editor.fontWeight": "400",
    "editor.minimap.enabled": false,
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "boundary",
    "editor.roundedSelection": true,
    "editor.rulers": [
        80
    ],
    "explorer.confirmDelete": false,
    "files.autoSave": "onFocusChange",
    "files.enableTrash": false,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.decorations.enabled": false,
    "git.enabled": false,
    "git.enableStatusBarSync": false,
    "problems.showCurrentInStatus": true,
    "python.formatting.blackArgs": [
        "-l",
        "80"
    ],
    "python.formatting.blackPath": "/usr/bin/black",
    "python.linting.pylintPath": "/usr/bin/pylint",
    "spellright.documentTypes": [
        "latex",
        "plaintext",
        "markdown"
    ],
    "spellright.language": [
        "en_US"
    ],
    "terminal.explorerKind": "external",
    "window.enableMenuBarMnemonics": false,
    "window.menuBarVisibility": "hidden",
    "window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
    "window.zoomLevel": 1,
    "workbench.activityBar.visible": false,
    "workbench.colorCustomizations": {
        "[Darcula]": {
            "editor.background": "#1E1E1E",
            "statusBar.background": "#1A1A1A",
            "statusBar.noFolderBackground": "#212121",
            "statusBar.debuggingBackground": "#263238"
        },
    },
    "workbench.editor.showTabs": false,
    "workbench.settings.openDefaultKeybindings": true,
    "workbench.settings.openDefaultSettings": true,
    "workbench.startupEditor": "none",
    "workbench.colorTheme": "Darcula",
    "vscode_custom_css.imports": ["file:///home/karelian/bla.css"],
    "vscode_custom_css.policy": true,
    "update.mode": "none",
    "editor.fontLigatures": null,
    "python.linting.pylintEnabled": true,
    "python.linting.enabled": true,
    "python.linting.pycodestyleEnabled": false,
    "python.pythonPath": "/usr/bin/python3.8",
    "python.formatting.provider": "black",
    "python.defaultInterpreterPath": "/usr/bin/python3.8",
    "python.diagnostics.sourceMapsEnabled": true,
    "python.linting.mypyEnabled": false,
    "python.linting.mypyPath": "/usr/bin/mypy",
    "python.linting.pylintUseMinimalCheckers": false,
    "python.linting.mypyArgs": [
        "--ignore-missing-imports",
        "--follow-imports=silent",
        "--show-column-numbers",
        "--dissalow-untyped-defs"
    ],
    "explorer.confirmDragAndDrop": false
}
