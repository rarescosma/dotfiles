#!/bin/bash

"${HOME}/.i3/wallpaper.sh"
dropbox &

nm-applet &
pa-applet &
parcellite &
autokey-gtk &

thunar --daemon &
syndaemon -i 1 -t -k &

# Special Eds
killall redshift
redshift-gtk -t 5500:4800 -l 59.32:18.06 & # Stockholm
#redshift-gtk -t 5500:4800 -l 46.77:23.62 & # Cluj-Napoca

kill "$(pgrep -f cloudy/watcher.py)"
"${HOME}/src/lang.python/cloudy/watcher.py" &

xcompmgr -cC -fF -n -D 5 -I 0.1 -O 0.1 &

wait
