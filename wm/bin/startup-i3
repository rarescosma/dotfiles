#!/bin/bash

"${HOME}/.i3/wallpaper.sh"
dropbox &

nm-applet &
pa-applet &
parcellite &
autokey-gtk &

thunar --daemon &
syndaemon -i 1 -t -k &

# Special Eds
killall redshift
redshift-gtk -t 5500:4800 -l 59.32:18.06 &
# Stockholm 59.32:18.06
# Cluj-Napoca 46.77:23.62

cloudy watch ${HOME}/Dropbox/Screenshots -c ${HOME}/.config/cloudy.yaml >/tmp/cloudy.log 2>&1 &

wait

