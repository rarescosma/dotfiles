# -- Ansible -------------------------------------------------------------------
ANSIBLE_NOCOWS=1

# -- AWS -----------------------------------------------------------------------
AWS_CREDENTIAL_FILE="${HOME}/.aws/credentials"
AWS_PROFILE="default"
EC2_REGION="eu-west-1"
_aws_zsh_completer_path="$HOME/.local/bin/aws_zsh_completer.sh"
[ -f $_aws_zsh_completer_path ] && source $_aws_zsh_completer_path
unset _aws_zsh_completer_path

# -- fzf! ----------------------------------------------------------------------
PATH="$PATH:$ZSH/../fzf/bin"
source "$ZSH/../fzf/shell/key-bindings.zsh"

# -- GNU Parallel --------------------------------------------------------------
. $(which env_parallel.zsh)

# -- K8s -----------------------------------------------------------------------
unset KUBECONFIG

# -- Memoize -------------------------------------------------------------------
[ -f "$ZSH/../memoize/memoize.sh" ] && source "$ZSH/../memoize/memoize.sh"

# -- Midnight Commander --------------------------------------------------------
MC_SKIN=$HOME/.config/mc/solarized.ini

# -- Z -------------------------------------------------------------------------
_Z_DATA=/var/tmp/karelian.z
